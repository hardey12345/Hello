#include <stdio.h>  
#include <conio.h>
 
 
int main()
{
    int arr[1000],i,n,sum=0;
   
    printf("Enter size of the array : ");
    scanf("%d",&n);
 
    for(i=1; i<9; i++)
    {
        printf("element %d : ",i);
        scanf("%d",&arr[i]);
    }
 
    
    for(i=0; i<9; i++)
    {
         
        sum+=arr[i];
    }
     printf("sum of all array element: %d",sum);
 
    return 0;
}
